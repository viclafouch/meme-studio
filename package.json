{
  "name": "meme-studio",
  "version": "1.0.0",
  "private": true,
  "description": "A complete meme generator coded in React / Express / Typescript",
  "scripts": {
    "dev": "npm-run-all --parallel dev:server dev:client",
    "dev:client": "webpack-dev-server --config ./build/webpack.client.js -d",
    "dev:server": "webpack --config ./build/webpack.server.js -d",
    "build": "npm-run-all --parallel build:client build:server",
    "build:client": "webpack --config ./build/webpack.client.js -p",
    "build:server": "webpack --config ./build/webpack.server.js -p",
    "start": "npx cross-env NODE_ENV=production node ./dist/server/index.js",
    "commit": "git-cz",
    "lint": "tsc --noEmit && eslint --ext .ts,.tsx src/",
    "ncu": "node tasks/ncu.js",
    "download": "node -r dotenv/config tasks/download.js",
    "postinstall": "cd src/client && npm install"
  },
  "engines": {
    "node": "12.16.x",
    "npm": "6.14.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viclafouch/meme-studio.git"
  },
  "keywords": [
    "typescript",
    "express",
    "react",
    "api",
    "react-hooks",
    "meme-generator"
  ],
  "author": "Victor de la Fouchardiere",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/viclafouch/meme-studio/issues"
  },
  "homepage": "https://github.com/viclafouch/meme-studio#readme",
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.9.6",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.9.6",
    "@babel/preset-env": "7.9.6",
    "@babel/preset-react": "7.9.4",
    "@babel/preset-typescript": "7.9.0",
    "@babel/runtime": "7.9.6",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@types/bluebird": "3.5.30",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.6",
    "@types/express": "4.17.6",
    "@types/helmet": "0.0.47",
    "@types/morgan": "1.9.0",
    "@types/node": "14.0.1",
    "@types/sequelize": "4.28.8",
    "@types/shortid": "0.0.29",
    "@types/twit": "2.2.24",
    "@types/validator": "13.0.0",
    "@typescript-eslint/eslint-plugin": "2.33.0",
    "@typescript-eslint/parser": "2.33.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "clean-webpack-plugin": "3.0.0",
    "commitizen": "4.1.2",
    "compression-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "5.1.1",
    "cross-env": "7.0.2",
    "css-loader": "3.5.3",
    "cz-conventional-changelog": "3.2.0",
    "dotenv-webpack": "1.8.0",
    "eslint": "7.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "html-loader": "1.1.0",
    "html-webpack-plugin": "4.3.0",
    "husky": "4.2.5",
    "image-size": "0.8.3",
    "markdown-loader": "5.1.0",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "4.14.1",
    "npm-check-updates": "5.0.0",
    "npm-run-all": "4.1.5",
    "preload-webpack-plugin": "3.0.0-beta.4",
    "prettier": "2.0.5",
    "sass-loader": "8.0.2",
    "source-map-loader": "0.2.4",
    "style-loader": "1.2.1",
    "tinify": "1.6.0-beta.2",
    "ts-loader": "7.0.4",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "typescript": "3.9.2",
    "webp-converter": "2.2.3",
    "webpack": "4.43.0",
    "webpack-bundle-analyzer": "3.8.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.0",
    "webpack-fix-style-only-entries": "0.4.0",
    "webpack-node-externals": "1.7.2",
    "webpack-shell-plugin": "0.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "cookie-parser": "1.4.5",
